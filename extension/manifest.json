{
  "name": "Boxcutter",
  "description": "Better package manifests on Github",
  "version": "8",
  "manifest_version": 2,
  "permissions": [
    "https://registry.npmjs.org/*",
    "https://rubygems.org/*",
    "https://hex.pm/*",
    "https://bower.herokuapp.com/*",
    "https://packagist.org/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
}